{
  "name": "beatsaber-bot-core",
  "description": "Beatsaber bot",
  "version": "0.3.0-rc.20",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": {
    "name": "ktKongTong",
    "email": "kt@ktlab.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./interface": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/interface/index.js",
      "import": "./dist/esm/interface/index.js"
    },
    "./interface/*": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/interface/**/*.js",
      "import": "./dist/esm/interface/**/*.js"
    },
    "./services": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/services/*.js",
      "import": "./dist/esm/services/*.js"
    },
    "./services/*": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/services/*",
      "import": "./dist/esm/services/*"
    },
    "./locales/*": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/locales/*",
      "import": "./dist/esm/locales/*"
    }
  },
  "keywords": [
    "beatsaber",
    "beatsaber-bot"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.840.0",
    "@ktfun/styled-qr-code-node": "^1.6.5",
    "@napi-rs/canvas": "^0.1.73",
    "chart.js": "^4.5.0",
    "country-flag-icons": "^1.5.19",
    "dayjs": "^1.11.13",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.525.0",
    "ofetch": "^1.4.1",
    "puppeteer-core": "^24.11.2",
    "react": "^19",
    "react-dom": "^19",
    "typeid-js": "1.2.0",
    "zod": "^3.25.71"
  },
  "scripts": {
    "db:gen": "drizzle-kit generate --config test/drizzle.config.ts",
    "fix-path-alias": " tsc-alias --outDir dist/cjs && tsc-alias --outDir dist/esm",
    "up": "tsup",
    "build": "tsup && pnpm run fix-path-alias",
    "pub": "pnpm run build && pnpm publish",
    "test": "vitest --silent=false --watch=false"
  },
  "devDependencies": {
    "js-yaml": "^4.1.0",
    "better-sqlite3": "^12.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "@faker-js/faker": "^9.9.0",
    "globby": "^14.0.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.3"
  }
}
