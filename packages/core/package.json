{
  "name": "beatsaber-bot-core",
  "description": "Beatsaber bot",
  "version": "0.3.0-rc.35",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": {
    "name": "ktKongTong",
    "email": "kt@ktlab.io"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js"
    },
    "./interface": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/interface/index.cjs",
      "import": "./dist/esm/interface/index.js"
    },
    "./interface/*": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/interface/**/*.cjs",
      "import": "./dist/esm/interface/**/*.js"
    },
    "./services": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/services/*.cjs",
      "import": "./dist/esm/services/*.js"
    },
    "./services/*": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/services/*",
      "import": "./dist/esm/services/*"
    },
    "./locales/*": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/locales/*",
      "import": "./dist/esm/locales/*"
    }
  },
  "keywords": [
    "beatsaber",
    "beatsaber-bot"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "@ktfun/styled-qr-code-node": "^1.6.6",
    "@napi-rs/canvas": "^0.1.74",
    "chart.js": "^4.5.0",
    "country-flag-icons": "^1.5.19",
    "dayjs": "^1.11.13",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.525.0",
    "ofetch": "^1.4.1",
    "puppeteer-core": "^24.14.0",
    "react": "^19",
    "react-dom": "^19",
    "typeid-js": "1.2.0",
    "zod": "^4.0.5"
  },
  "scripts": {
    "ci:publish-dev": "pnpm publish --tag dev --no-git-checks",
    "db:gen": "drizzle-kit generate --config test/drizzle.config.ts",
    "db:mi": "drizzle-kit migrate --config test/drizzle.config.ts",
    "fix-path-alias": " tsc-alias --outDir dist/cjs --resolve-full-extension=.cjs && tsc-alias --outDir dist/esm --resolve-full-extension=.js",
    "up": "tsup",
    "tsc": "tsc",
    "build": "tsup && pnpm run fix-path-alias",
    "pub": "pnpm run build && pnpm publish",
    "test": "vitest --silent=false --watch=false",
    "db:studio": "drizzle-kit studio --config test/drizzle.config.ts",
    "story:init": "storybook init",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@faker-js/faker": "^9.9.0",
    "@storybook/addon-a11y": "^9.0.17",
    "@storybook/addon-docs": "^9.0.17",
    "@storybook/addon-onboarding": "^9.0.17",
    "@storybook/addon-vitest": "^9.0.17",
    "@storybook/react-vite": "^9.0.17",
    "@tailwindcss/vite": "^4.1.11",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "better-sqlite3": "^12.2.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.3",
    "eslint-plugin-storybook": "^9.0.17",
    "globby": "^14.0.1",
    "js-yaml": "^4.1.0",
    "playwright": "^1.54.1",
    "storybook": "^9.0.17",
    "tailwindcss": "^4.1.11",
    "vite": "^7.0.5",
    "@vitejs/plugin-react": "^4.7.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
