{
  "name": "beatsaber-bot-core",
  "description": "Beatsaber bot",
  "version": "0.2.0-rc.3",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": {
    "name": "ktKongTong",
    "email": "kt@ktlab.io"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./interface": {
      "require": "./dist/cjs/interface/index.js",
      "types": "./dist/types/interface/index.d.ts"
    },
    "./interface/*": {
      "require": "./dist/cjs/interface/**/*.js",
      "types": "./dist/types/interface/**/*.d.ts"
    },
    "./service": {
      "require": "./dist/cjs/service/*.js",
      "types": "./dist/types/service/*.d.ts"
    },
    "./service/*": {
      "require": "./dist/cjs/service/*/index.js",
      "types": "./dist/types/service/*/index.d.ts"
    },
    "./infra": {
      "require": "./dist/cjs/infra/index.js",
      "types": "./dist/types/infra/index.d.ts"
    },
    "./infra/*": {
      "require": "./dist/cjs/infra/*/index.js",
      "types": "./dist/types/infra/*/index.d.ts"
    }
  },
  "keywords": [
    "beatsaber",
    "beatsaber-bot"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.658.1",
    "@ktfun/styled-qr-code-node": "^1.6.5",
    "chart.js": "^4.4.3",
    "country-flag-icons": "^1.5.11",
    "dayjs": "^1.11.3",
    "lru-cache": "^11.0.1",
    "lucide-react": "^0.379.0",
    "ofetch": "^1.4.1",
    "puppeteer-core": "^24.11.1",
    "react": "^19",
    "react-dom": "^19",
    "@napi-rs/canvas": "^0.1.72"
  },
  "scripts": {
    "copy-yml-resource": "node scripts/build.mjs",
    "build": "tsup && pnpm run copy-yml-resource",
    "pub": "pnpm run build && pnpm publish"
  }
}
