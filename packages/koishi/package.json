{
  "name": "koishi-plugin-beatsaber-bot",
  "description": "一个 用于 BeatSaber 的 koishi Bot 插件",
  "version": "0.3.0-rc.21",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": {
    "name": "ktKongTong",
    "email": "kt@ktlab.io"
  },
  "repository": "https://github.com/ktKongTong/koishi-beats-bot",
  "homepage": "https://koishi-beats-bot.vercel.app/",
  "keywords": [
    "beatsaber",
    "beatsaber-bot",
    "koishi",
    "koishi-plugin"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "koishi": {
    "service": {
      "required": [
        "database",
        "cron"
      ],
      "optional": [
        "puppeteer"
      ]
    },
    "locales": [
      "en",
      "zh"
    ],
    "description": {
      "en": "a koishi bot plugin for BeatSaber",
      "zh": "一个 BeatSaber Bot 插件"
    }
  },
  "devDependencies": {
    "@koishijs/plugin-adapter-qq": "4.6.2",
    "@koishijs/plugin-console": "^5.30.9",
    "@koishijs/plugin-database-sqlite": "^4.6.0",
    "@koishijs/plugin-echo": "^2.2.5",
    "@koishijs/plugin-http": "^0.6.3",
    "@koishijs/plugin-logger": "^2.6.9",
    "@koishijs/plugin-mock": "^2.6.6",
    "@koishijs/plugin-sandbox": "^3.4.2",
    "@satorijs/adapter-qq": "4.10.1",
    "@types/node": "^24.0.10",
    "beatsaber-bot-core": "workspace:*",
    "esbuild-register": "^3.6.0",
    "koishi": "4.17.7",
    "koishi-plugin-adapter-onebot": "6.8.0",
    "koishi-plugin-cron": "^3.1.0",
    "koishi-plugin-downloads": "0.1.3",
    "koishi-plugin-ffmpeg": "1.1.0",
    "koishi-plugin-puppeteer": "^3.9.0",
    "koishi-plugin-silk": "1.3.1"
  },
  "peerDependencies": {
    "beatsaber-bot-core": "workspace:*",
    "koishi": "^4.17.7",
    "koishi-plugin-cron": "^3.1.0",
    "koishi-plugin-puppeteer": "^3.7.3",
    "typeid-js": "^1.2.0"
  },
  "scripts": {
    "typecheck": "tsc",
    "build": "tsc -p tsconfig.build.json  && tsc-alias",
    "clean": "rm -rf lib  tsconfig.tsbuildinfo"
  }
}
